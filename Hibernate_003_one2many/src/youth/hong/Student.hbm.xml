<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="youth.hong.Grade" table="grade">
   		<id name="sid" type="int">
   			<column name="sid" />
   			<generator class="increment" />
   		</id>
   	<property name="sname" type="java.lang.String" not-null="true">
   		<column name="sname" />
   	</property>
   	<property name="sex" type="java.lang.Character">
   		<column name="sex" />
   	</property>
   
   	<property name="gid" type="java.lang.Integer">
   		<column name="gid" />
   	</property>
   	
   </class>
</hibernate-mapping>